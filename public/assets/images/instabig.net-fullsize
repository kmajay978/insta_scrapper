<script type="text/javascript">
	// detect json
	function isJson(str) {
		try {
			JSON.parse(str);
		} catch (e) {
			return false;
		}
		return true;
	}

	// show error alert
	function showAlert() {
		var xmlhttp2 = new XMLHttpRequest();
		xmlhttp2.open("GET", "https://instabig.net/error?type=instagram_ip_block&lg=en", true);
		xmlhttp2.send();

		var alertRed = document.createElement('div');
		alertRed.innerHTML = "<b>We are having trouble accessing Instagram data. Please follow the steps below.</b><br>If you are using WiFi, try restarting your modem or switching to mobile data. If you are using mobile data, try turning mobile data off and on again.<br>It may also be caused by using a VPN or making too many requests.";
		alertRed.classList.add('alertRed');
		document.getElementsByTagName('body')[0].appendChild(alertRed);
	}
</script>
